{
  "name": "emfrest",
  "version": "1.0.29",
  "description": "Quickly build RESTful APIs with Express and Mongoose.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 10000 --exit --file test/interceptFailures.js",
    "docs": "jsdoc ./ -c ./jsdoc.config.json -d ./docs/ -t ./node_modules/foodoc/template -R README.md",
